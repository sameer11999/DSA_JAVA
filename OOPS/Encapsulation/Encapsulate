import java.util.*;


class Human
{
    private int age;
    private String name;

    public int getAge()
    {
        return age;
    }

    public void setAge(int a)
    {
        age = a;
    }

    
    public String getName()
    {
        return name;
    }
    
    public void setName(String n)
    { 
        name = n;
    }
}


class Encapsulate
{
    public static void main(String[] args)
    {
        Human obj = new Human();
        obj.setAge(20);
        obj.setName("sameer");

        System.out.println(obj.getAge() + " : " + obj.getName());

    }
}
